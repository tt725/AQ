{
  "env": [
    {
      "name": ["Asterix-MinAtar-v0"],
      "train_steps": [3e6],
      "test_per_episodes": [100],
      "input_type": ["pixel"]
    }
  ],
  "agent": [
    {
      "name": ["DQN", "DDQN"]
    },
     {
      "name": ["MaxminDQN", "AveragedDQN"],
      "target_networks_num": [2]
    },
    {
      "name": ["SCDQN"],
      "scq_parameter": [2.0]
    },
    {
      "name": ["SoftmaxDQN"],
      "SoftQ_parameter": [1]
    },
    {
      "name": ["WDQN"],
      "weight_parameter": [1.0]
    },
    {
      "name": ["REDQN", "EBDQN", "AdaEDQN"],
      "target_networks_num": [10]
    },
    {
      "name": ["AlterDQN"],
      "QL_num": [1,2,4,8,16],
      "DQL_num": [4]
    },
    {
      "name": ["AlterDQN"],
      "QL_num": [4],
      "DQL_num": [1,2,8,16]
    },
    {
      "name": ["SoftAlterDQN"],
      "soft_parameter": [1,2,5,10,100]
    }
  ],
  "device": ["cuda"],
  "feature_dim": [128],
  "hidden_layers": [[]],
  "memory_type": ["Replay"],
  "memory_size": [1e5],
  "exploration_type": ["LinearEpsilonGreedy"],
  "exploration_steps": [5e3],
  "epsilon_steps": [1e5],
  "epsilon_start": [1.0],
  "epsilon_end": [0.1],
  "loss": ["SmoothL1Loss"],
  "optimizer": [
    {
      "name": ["RMSprop"],
      "kwargs": [{"lr": [1e-2], "alpha": [0.95], "centered": [true], "eps": [0.01]}]
    }
  ],
  "batch_size": [32],
  "display_interval": [1000],
  "rolling_score_window": [{"Train": [100], "Test": [10]}],
  "discount": [0.99],
  "seed": [1],
  "show_tb": [false],
  "gradient_clip": [-1],
  "target_network_update_freqency": [1000],
  "sgd_update_frequency": [1],
  "generate_random_seed": [true]
}
